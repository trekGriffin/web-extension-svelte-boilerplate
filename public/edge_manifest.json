{
  "name": "proxy",
  "version": "24.90108.1",
  "manifest_version": 3,
  "description": "unable to browse in certain environments? use this proxy.",
  "icons": {
      "48": "images/splash.png"
  },
  "action": {
      "default_popup": "popup.html"
  },
  "declarative_net_request" : {
      "rule_resources" : [{
        "id": "ruleset_1",
        "enabled": true,
        "path": "images/rules_1.json"
      }
      ]},

  "permissions": [
      "proxy",
      "storage",
      "declarativeNetRequest"
    ]

}